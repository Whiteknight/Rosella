namespace Rosella { namespace Dumper
{
    class DumpHandler
    {
        function DumpHandler() { }

        function can_dump(var obj)
        {
            return 1;
        }

        function dump(var dump, var emit, var obj)
        {
            try {
                string text = string(obj);
                emit.data("(%s): '%s'\n", typeof(obj), text);
            } catch() {
                emit.data("PMC (%s)\n", typeof(obj));
            }
        }
    }
}}
