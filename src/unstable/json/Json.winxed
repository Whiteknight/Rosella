namespace Rosella.Json
{
    function to_json(var obj)
    {
        var dumper = new Rosella.Json.Dumper();
        return dumper.dump(obj);
    }

    function parse(string json)
    {
        int len = length(json);
        var b = get_iterator(var(json));
        int s[] = [];
        return Rosella.Json.Parser.__parse_value(json, s, b, len);
    }
}
