namespace Rosella { namespace String { namespace Tokenizer
{
    class Token
    {
        var data;
        var metadata;

        function Token(var data, var metadata)
        {
            self.data = data;
            self.metadata = metadata;
        }

        function data()
        {
            return self.data;
        }

        function metadata()
        {
            return self.metadata;
        }
    }
}}}
