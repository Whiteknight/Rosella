$loadlib "trans_ops";
$loadlib "sys_ops";

/* inline helpers
*/

// Natural logarithm
inline ln(float val) return float
{
    float result;
    ${ ln result, val };
    return result;
}

inline intval_max() return int
{
    int value;
    ${ sysinfo value, 9 };
    return value;
}

inline intval_min() return int
{
    int value;
    ${ sysinfo value, 10 };
    return value;
}

