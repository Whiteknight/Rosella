class Rosella.Net.Uri
{
    var uri;
    function Uri(string uri)
    {
        self.uri = uri;
    }

    function uri()
    {
        return string(self.uri);
    }

    function protocol()
    {
        return string(split(":", string(self.uri))[0]);
    }
}
