class Rosella.Xml.Text
{
    var text;
    function Text(string text)
    {
        self.text = text;
    }

    function get_text() { return self.text; }

    function to_xml()
    {
        var sb = new 'StringBuilder';
        self.to_xml_sb(sb);
        return sb;
    }

    function to_xml_sb(var sb)
    {
        push(sb, self.text);
    }
}
